/**home.wxss**/
.suggestion{}
.suggestion .head{
  width:100%;
  display: flex;
  flex-direction: row;
  align-items: left;
  justify-content: center;
}
.suggestion .head input{
  width:100%;
}

.suggestion .suggestion-list{
  width: 90%;
  margin:auto;
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 10rpx 0;
}
.suggestion-list .candidate{
  width:100%;
  margin:8rpx;
  background:#FFFFFF;
  padding:2rpx;
  border-radius: 12rpx;
}
.suggestion-list .candidate .title{
  margin:auto 8rpx;
  font-size: 36rpx;
  font-weight: 600;
}
.suggestion-list .candidate .address{
  margin:auto 8rpx;
  font-size:24rpx;
  font-weight: 200;
}
